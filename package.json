{
  "name": "genshin-ts",
  "version": "0.1.1",
  "description": "Use Typescript to develop Genshin UGC - Miliastra Wonderland",
  "type": "module",
  "author": "josStorer <josstorer@outlook.com> (https://github.com/josStorer)",
  "homepage": "https://gsts.moe",
  "repository": {
    "type": "git",
    "url": "https://github.com/josStorer/genshin-ts"
  },
  "keywords": [
    "genshin",
    "genshin-ts",
    "typescript",
    "compiler"
  ],
  "license": "MIT",
  "bin": {
    "gsts": "bin/gsts.mjs"
  },
  "main": "./dist/src/index.js",
  "types": "./types/gsts/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/gsts/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./eslint": {
      "types": "./dist/src/eslint/index.d.ts",
      "default": "./dist/src/eslint/index.js"
    },
    "./eslint.globals.mjs": "./eslint.globals.mjs",
    "./compiler/*": {
      "types": "./dist/src/compiler/*.d.ts",
      "default": "./dist/src/compiler/*.js"
    },
    "./injector/*": {
      "types": "./dist/src/injector/*.d.ts",
      "default": "./dist/src/injector/*.js"
    },
    "./runtime/*": {
      "types": "./dist/src/runtime/*.d.ts",
      "default": "./dist/src/runtime/*.js"
    },
    "./definitions/*": {
      "types": "./dist/src/definitions/*.d.ts",
      "default": "./dist/src/definitions/*.js"
    },
    "./configs/*": "./configs/*",
    "./tsconfig/*": "./configs/tsconfig/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/src",
    "types",
    "configs",
    "eslint.globals.mjs",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "pretest": "node scripts/clean-tests.mjs",
    "test": "npm run build && tsx scripts/generate-node-gia-tests.ts && tsx scripts/generate-enum-gia-tests.ts && node ./bin/gsts.mjs -c gsts.test.config.ts",
    "prebuild": "node -e \"require('node:fs').rmSync('dist',{recursive:true,force:true})\"",
    "build": "tsc -p tsconfig.json",
    "postbuild": "node scripts/postbuild.mjs",
    "pack": "npm run build && npm pack",
    "gen": "tsx ./scripts/generate-definitions.ts && prettier --write src/definitions/**/*.ts",
    "to-gs": "tsx ./src/compiler/ts_to_gs.ts",
    "example": "npm run build && node ./bin/gsts.mjs",
    "dev": "npm run build && node ./bin/gsts.mjs dev"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@types/node": "^24.10.1",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.6.2",
    "chokidar": "^3.6.0",
    "commander": "^14.0.2",
    "fast-glob": "^3.3.3",
    "i18next": "^25.3.2",
    "os-locale": "^6.0.0",
    "picocolors": "^1.1.1",
    "protobufjs": "^7.5.4",
    "semver": "^7.7.2",
    "tsx": "^4.20.6",
    "type-fest": "^5.3.1",
    "typescript": "^5.9.3",
    "undici": "^7.4.0",
    "yaml": "^2.8.0",
    "zod": "^4.1.12"
  }
}
